"CFLAGS   += $(shell $(JL_SHARE)/julia-config.jl --cflags)\n"
"CXXFLAGS += $(shell $(JL_SHARE)/julia-config.jl --cflags)\n"
"LDFLAGS  += $(shell $(JL_SHARE)/julia-config.jl --ldflags)\n"
"LDLIBS   += $(shell $(JL_SHARE)/julia-config.jl --ldlibs)\n"
